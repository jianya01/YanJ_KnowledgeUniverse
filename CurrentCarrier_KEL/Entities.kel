        // STRING TransactionID),
    // MODEL(UID, TransactionID));

Report := ENTITY(FLAT(UID(TransactionID),
		// CrossProductReport rCrossProductReport = UID(TransactionID),
		DATE DateOfReport,
		STRING TransactionID,
		STRING DataSource),
    MODEL(UID,
		// rCrossProductReport,
		DateOfReport,
		TransactionID,
		DataSource));

Policy := ENTITY(FLAT(UID(TransactionID, InsuranceType, Ambest, PolicyNumber),
