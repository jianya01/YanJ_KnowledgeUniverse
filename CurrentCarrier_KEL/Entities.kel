Holder := ENTITY(FLAT(UID(LexID, TransactionID, InsuranceType, Ambest, PolicyNumber, LastName, FirstName, MiddleName, NameSuffix, DateOfBirth, PrimRange, Predir, PrimName, AddressSuffix, PostDir, UnitDesig, SecondaryRange, City, Zip5),
